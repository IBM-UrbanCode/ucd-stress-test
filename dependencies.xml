<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.urbancode.com/schema/cs/project-dependencies/1.0"
 name="ucd-create-app" workflow="build workflow [master]">
  <dependencies conflictStrategy="FAIL">
    <!-- Dependencies Here -->
    <!-- Example Anthill Project Dependency
    <dependency>
      <ah-project name="Anthill Project" workflow="build workflow [master]"/>
      <criteria status="success"/>
      <delivery artifact-set="Library">
        <directory>lib</directory>
      </delivery>
    </dependency>
    -->
    <!-- Example Codestation Project Dependency
    <dependency>
